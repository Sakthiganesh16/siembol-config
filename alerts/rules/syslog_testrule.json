{
  "rule_name": "syslog_testrule",
  "rule_author": "siembol",
  "rule_version": 1,
  "rule_description": "Rule for checking the test_syslog config",
  "source_type": "test_syslog",
  "matchers": [
    {
      "is_enabled": true,
      "description": "sudo ",
      "matcher_type": "IS_IN_SET",
      "is_negated": false,
      "field": "syslog_hostname",
      "case_insensitive": false,
      "data": "mymachine.example.com"
    }
  ]
}