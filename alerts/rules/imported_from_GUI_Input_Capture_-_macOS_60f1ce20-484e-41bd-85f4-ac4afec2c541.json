{
  "rule_name" : "imported_from_GUI_Input_Capture_-_macOS_60f1ce20-484e-41bd-85f4-ac4afec2c541",
  "rule_author" : "siembol",
  "rule_version": 1,
  "rule_description" : "Detects attempts to use system dialog prompts to capture user credentials",
  "source_type" : "macos",
  "matchers" : [ {
    "field" : "Image",
    "data" : "\\Q/usr/sbin/osascript\\E",
    "is_enabled" : true,
    "matcher_type" : "REGEX_MATCH",
    "is_negated" : false,
    "case_insensitive" : false,
    "starts_with" : false,
    "ends_with" : false
  }, {
    "field" : "CommandLine",
    "data" : ".*\\Qadmin\\E.*|.*\\Qadministrator\\E.*|.*\\Qauthenticate\\E.*|.*\\Qauthentication\\E.*|.*\\Qcredentials\\E.*|.*\\Qpass\\E.*|.*\\Qpassword\\E.*|.*\\Qunlock\\E.*",
    "is_enabled" : true,
    "matcher_type" : "REGEX_MATCH",
    "is_negated" : false,
    "case_insensitive" : false,
    "starts_with" : false,
    "ends_with" : false
  }, {
    "field" : "CommandLine",
    "data" : "(?=.*\\Q-e\\E.*)(?=.*\\Qdisplay\\E.*)(?=.*\\Qdialog\\E.*)(?=.*\\Qanswer\\E.*)",
    "is_enabled" : true,
    "matcher_type" : "REGEX_MATCH",
    "is_negated" : false,
    "case_insensitive" : false,
    "starts_with" : false,
    "ends_with" : false
  } ],
  "tags" : [ {
    "tag_name" : "sigma_id",
    "tag_value" : "60f1ce20-484e-41bd-85f4-ac4afec2c541"
  }, {
    "tag_name" : "sigma_tags",
    "tag_value" : "[attack.credential_access, attack.t1056.002]"
  }, {
    "tag_name" : "sigma_fp",
    "tag_value" : "[Legitimate administration tools and activities]"
  } ],
  "rule_protection" : {
    "max_per_hour" : 30,
    "max_per_day" : 100
  }
}