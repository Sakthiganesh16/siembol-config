{
  "rule_name": "linux_ruleAlert",
  "rule_author": "siembol",
  "rule_version": 2,
  "rule_description": "This rule is for checking the linux_system config",
  "source_type": "linux_system",
  "matchers": [
    {
      "is_enabled": true,
      "matcher_type": "CONTAINS",
      "is_negated": false,
      "field": "OS",
      "case_insensitive": true,
      "starts_with": false,
      "ends_with": false,
      "data": "linux"
    }
  ],
  "rule_protection": {
    "max_per_hour": 30,
    "max_per_day": 100
  }
}