      "is_negated" : false,
      "case_insensitive" : false,
      "starts_with" : false,
      "ends_with" : false
    }, {
      "field" : "CommandLine",
      "data" : ".*\\Q-e\\E.*",
      "is_enabled" : true,
      "matcher_type" : "REGEX_MATCH",
      "is_negated" : false,
      "case_insensitive" : false,
      "starts_with" : false,
