{
  "rule_name" : "imported_from_Indicator_Removal_on_Host_-_Clear_Mac_System_Logs_acf61bd8-d814-4272-81f0-a7a269aa69aa",
  "rule_author" : "siembol",
  "rule_version": 1,
  "rule_description" : "Detects deletion of local audit logs",
  "source_type" : "macos",
  "matchers" : [ {
    "field" : "Image",
    "data" : ".*\\Q/rm\\E$|.*\\Q/unlink\\E$|.*\\Q/shred\\E$",
    "is_enabled" : true,
    "matcher_type" : "REGEX_MATCH",
    "is_negated" : false,
    "case_insensitive" : false,
    "starts_with" : false,
    "ends_with" : false
  }, {
    "matchers" : [ {
      "field" : "CommandLine",
      "data" : ".*\\Q/var/log\\E.*",
      "is_enabled" : true,
      "matcher_type" : "REGEX_MATCH",
      "is_negated" : false,
      "case_insensitive" : false,
      "starts_with" : false,
      "ends_with" : false
    }, {
      "field" : "CommandLine",
      "data" : "(?=.*\\Q/Users/\\E.*)(?=.*\\Q/Library/Logs/\\E.*)",
      "is_enabled" : true,
      "matcher_type" : "REGEX_MATCH",
      "is_negated" : false,
      "case_insensitive" : false,
      "starts_with" : false,
      "ends_with" : false
    } ],
    "is_enabled" : true,
    "matcher_type" : "COMPOSITE_OR",
    "is_negated" : false,
    "case_insensitive" : false,
    "starts_with" : false,
    "ends_with" : false
  } ],
  "tags" : [ {
